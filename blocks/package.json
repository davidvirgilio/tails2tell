{
  "name": "blocks",
  "version": "0.1.0",
  "description": "Custom blocks for tailstotell.ca",
  "author": "David Virgilio",
  "license": "GPL-2.0-or-later",
  "main": "build/index.js",
  "scripts": {
    "build": "wp-scripts build",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "packages-update": "wp-scripts packages-update",
    "plugin-zip": "wp-scripts plugin-zip",
    "start": "npm-run-all start:*",
    "start:decorative-banner": "wp-scripts start --webpack-src-dir=decorative-banner/src --output-path=decorative-banner/build",
    "start:adoption-categories": "wp-scripts start --webpack-src-dir=adoption-categories/src --output-path=adoption-categories/build",
    "start:hero": "wp-scripts start --webpack-src-dir=hero/src --output-path=hero/build",
    "start:section": "wp-scripts start --webpack-src-dir=section/src --output-path=section/build",
    "start:separator": "wp-scripts start --webpack-src-dir=separator/src --output-path=separator/build",
    "start:image-frame": "wp-scripts start --webpack-src-dir=image-frame/src --output-path=image-frame/build",
    "start:slider-members": "wp-scripts start --webpack-src-dir=slider-members/src --output-path=slider-members/build",
    "start:button": "wp-scripts start --webpack-src-dir=button/src --output-path=button/build",
    "start:swiper":"wp-scripts start --webpack-src-dir=swiper/src --output-path=swiper/build"
  },
  "devDependencies": {
    "@wordpress/scripts": "^30.4.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@wordpress/icons": "^10.11.0",
    "swiper": "^11.1.14"
  }
}
