{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tails2tell/section",
	"version": "0.1.0",
	"title": "Section",
	"category": "design",
	"icon": "feedback",
	"description": "Block designed for sections of content.",
	"example": {},
	"supports": {
		"html": false,
		"spacing": {
			"margin":true
		}
	},
	"textdomain": "section",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"attributes": {
		"isPrimary":{
			"type":"boolean",
			"default": false
		},
		"hasStyle":{
			"type": "boolean",
			"default": true
		}, 
		"hasBottom":{
			"type": "boolean",
			"default": true
		},
		"maxWidth":{
			"type": "string",
			"default": "1100px"
		},
		"padding":{
			"type": "object",
			"default":{
				"inline": "3rem",
				"block": "1rem"
			}
		}
	}
}
